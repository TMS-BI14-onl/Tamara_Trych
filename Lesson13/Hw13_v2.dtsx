                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT TOP 10 p.Title AS product_name, s.Name AS shop_name, d.EmployeeNumber, 
	a.PaymentType, c.Passport, CAST(GETDATE()-(FLOOR(RAND(CHECKSUM(NEWID()))*(90))+1) AS DATE) AS SaleDate,
	FLOOR(RAND(CHECKSUM(NEWID()))*(10))+1 AS Amount,
	ROUND(RAND(CHECKSUM(NEWID()))*(2000-40+1)+40, 2) AS Price 	
FROM Product p JOIN Shop s ON (p.ID/10+FLOOR(RAND(CHECKSUM(NEWID()))*(900))+0) = s.ID
	JOIN Seller d ON (p.ID+FLOOR(RAND(CHECKSUM(NEWID()))*(2000))+0) = d.ID
	JOIN PaymentType a ON (p.ID%3+1) = a.ID
	JOIN Customer c ON (p.ID+FLOOR(RAND(CHECKSUM(NEWID()))*(99000))+0) = c.ID</property>
